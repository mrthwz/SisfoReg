package tubes;

/**
 *
 * @author Adrian G Nurcahyo
 */
public class mahasiswa1 {
    private String namaMhs;
    private String emailMhs;
    private String noIndukMhs;
    private String alamatMhs;

    public mahasiswa1(String namaMhs, String emailMhs, String noIndukMhs, String alamatMhs) {
        this.namaMhs = namaMhs;
        this.emailMhs = emailMhs;
        this.noIndukMhs = noIndukMhs;
        this.alamatMhs = alamatMhs;
    }

    public String getNamaMhs() {
        return namaMhs;
    }

    public String getEmailMhs() {
        return emailMhs;
    }

    public String getNoIndukMhs() {
        return noIndukMhs;
    }

    public String getAlamatMhs() {
        return alamatMhs;
    }
    
    
    
}
